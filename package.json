{
  "name": "skolverket-styleguide",
  "version": "1.2.0",
  "engines": {
    "node": "^8.12.0",
    "yarn": "^1.10.0"
  },
  "homepage": "https://skolverket.github.io/styleguide",
  "repository": "https://github.com/skolverket/styleguide",
  "scripts": {
    "dev": "parallelshell \"fractal start --sync\" \"gulp -s\"",
    "prebuild": "node gulp/fractal-patch/patch.js",
    "build": "gulp -s && fractal build",
    "deploy": "NODE_ENV=production yarn build && gh-pages -d site",
    "postinstall": "node gulp/fractal-patch/patch.js",
    "prerelease": "NODE_ENV=production gulp",
    "release": "dotenv release-it --",
    "postrelease": "rm skolverket-*.zip"
  },
  "dependencies": {
    "@frctl/fractal": "^1.1.7",
    "babel-core": "~6.26.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-stage-2": "~6.24.1",
    "babelify": "~8.0.0",
    "browserify": "~14.5.0",
    "classlist-polyfill": "^1.2.0",
    "del": "~3.0.0",
    "dotenv-cli": "^1.4.0",
    "focus-trap": "^4.0.2",
    "gh-pages": "^1.1.0",
    "gulp": "~3.9.1",
    "gulp-autoprefixer": "~4.0.0",
    "gulp-environments": "~0.1.2",
    "gulp-flatten": "~0.3.1",
    "gulp-rename": "~1.2.2",
    "gulp-replace": "~0.6.1",
    "gulp-sass": "~3.1.0",
    "gulp-sequence": "~0.4.6",
    "gulp-sourcemaps": "~2.6.1",
    "gulp-svgmin": "~1.2.4",
    "gulp-svgstore": "~6.1.0",
    "gulp-uglify": "~3.0.0",
    "parallelshell": "~3.0.2",
    "release-it": "^8.2.0",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.9.0"
  },
  "devDependencies": {
    "node-zip": "^1.1.1"
  }
}
